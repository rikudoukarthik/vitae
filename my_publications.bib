@article{saino2004,
  title = {Ecological Conditions during Winter Predict Arrival Date at the Breeding Quarters in a Trans-{{Saharan}} Migratory Bird},
  author = {Saino, Nicola and Sz{\'e}p, Tibor and Romano, Maria and Rubolini, Diego and Spina, Fernando and M{\o}ller, Anders Pape},
