@article{nosil2018,
  title = {Natural Selection and the Predictability of Evolution in {{Timema}} Stick Insects},
  author = {Nosil, Patrik and Villoutreix, Romain and de Carvalho, Clarissa F. and Farkas, Timothy E. and {Soria-Carrasco}, V{\'i}ctor and Feder, Jeffrey L. and Crespi, Bernard J. and Gompert, Zach},
  year = {2018},
