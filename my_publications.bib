@article{freeman2014,
  title = {Rapid Upslope Shifts in {{New Guinean}} Birds Illustrate Strong Distributional Responses of Tropical Montane Species to Global Warming},
  author = {Freeman, Benjamin G. and Class Freeman, Alexandra M.},
  year = {2014},
  month = mar,
  journal = {Proceedings of the National Academy of Sciences},
  volume = {111},
