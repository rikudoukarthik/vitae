@article{paudelPopulationTrendsHimalayan2016,
  title = {Population Trends in {{Himalayan Griffon}} in {{Upper Mustang}}, {{Nepal}}, before and after the Ban on Diclofenac},
  author = {Paudel, Khadananda and Amano, Tatsuya and Acharya, Raju and Chaudhary, Anand and Baral, Hem S. and Bhusal, Krishna P. and Chaudhary, Ishwari P. and Green, Rhys E. and Cuthbert, Richard J. and Galligan, Toby H.},
