@article{guthula2022,
  title = {Biodiversity Significance of Small Habitat Patches: {{More}} than Half of {{Indian}} Bird Species Are in Academic Campuses},
  shorttitle = {Biodiversity Significance of Small Habitat Patches},
  author = {Guthula, Venkanna Babu and Shrotriya, Shivam and Nigam, Parag and Goyal, Surendra Prakash and Mohan, Dhananjai and Habib, Bilal},
