@article{feeny1970,
  title = {Seasonal Changes in Oak Leaf Tannins and Nutrients as a Cause of Spring Feeding by Winter Moth Caterpillars},
  author = {Feeny, Paul},
  year = {1970},
  journal = {Ecology},
