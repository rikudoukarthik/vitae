@article{li2016,
  title = {Remotely {{Sensed Data Informs Red List Evaluations}} and {{Conservation Priorities}} in {{Southeast Asia}}},
  author = {Li, Binbin V. and Hughes, Alice C. and Jenkins, Clinton N. and {Ocampo-Pe{\~n}uela}, Natalia and Pimm, Stuart L.},
