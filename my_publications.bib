@article{olbergPreySizeSelection2005,
  title = {Prey Size Selection and Distance Estimation in Foraging Adult Dragonflies},
  author = {Olberg, R. M. and Worthington, A. H. and Fox, J. L. and Bessette, C. E. and Loosemore, M. P.},
  year = {2005},
  month = sep,
  journal = {Journal of Comparative Physiology A},
  volume = {191},
  number = {9},
