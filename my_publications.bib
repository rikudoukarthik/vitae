@article{papageorgiouHabitatSelectionWoodchat2017,
  title = {Habitat Selection of Woodchat Shrikes {{Lanius}} Senator during Spring Stopover Is Related to Foraging Strategy},
  author = {Papageorgiou, Danai and Barboutis, Christos and Kassara, Christina and Giokas, Sinos},
