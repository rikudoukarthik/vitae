@article{jonsson2012,
  title = {Ecological and Evolutionary Determinants for the Adaptive Radiation of the {{Madagascan}} Vangas},
  author = {J{\o}nsson, Knud A. and Fabre, Pierre-Henri and Fritz, Susanne A. and Etienne, Rampal S. and Ricklefs, Robert E. and J{\o}rgensen, Tobias B. and Fjelds{\aa}, Jon and Rahbek, Carsten and Ericson, Per G. P. and Woog, Friederike and Pasquet, Eric and Irestedt, Martin},
