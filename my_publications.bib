@article{naef-daenzer2000,
  title = {Prey Selection and Foraging Performance of Breeding {{Great Tits Parus}} Major in Relation to Food Availability},
  author = {Naef-Daenzer, Luzia and Naef-Daenzer, Beat and Nager, Ruedi G.},
  year = {2000},
