@article{nagy2004,
  title = {Factors Influencing Fecundity in Migratory Songbirds: {{Is}} Nest Predation the Most Important?},
  shorttitle = {Factors Influencing Fecundity in Migratory Songbirds},
  author = {Nagy, Laura R. and Holmes, Richard T.},
