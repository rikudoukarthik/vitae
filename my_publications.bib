  title = {Thermoregulatory Constraints on Behavior: Patterns in a Neotropical Dragonfly Assemblage},
  shorttitle = {Thermoregulatory Constraints on Behavior},
  author = {De Marco Jr, Paulo and Latini, Anderson O. and Resende, Daniela C.},
  year = {2005},
