@article{bond2007,
  title = {The Evolution of Color Polymorphism: {{Crypticity}}, Searching Images, and Apostatic Selection},
  shorttitle = {The {{Evolution}} of {{Color Polymorphism}}},
  author = {Bond, Alan B.},
