@article{marsh2021,
  title = {Where Birds Felt Louder: {{The}} Garden as a Refuge during {{COVID-19}}},
  shorttitle = {Where Birds Felt Louder},
  author = {Marsh, Pauline and Diekmann, Lucy O and Egerer, Monika and Lin, Brenda and Ossola, Alessandro and Kingsley, Jonathan},
