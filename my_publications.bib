@article{winger2019,
  title = {Nocturnal Flight-Calling Behaviour Predicts Vulnerability to Artificial Light in Migratory Birds},
  author = {Winger, Benjamin M. and Weeks, Brian C. and Farnsworth, Andrew and Jones, Andrew W. and Hennen, Mary and Willard, David E.},
  year = {2019},
