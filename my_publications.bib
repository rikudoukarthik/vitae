  langid = {english},
  pmid = {24006468},
  file = {C\:\\Users\\Karthik Thrikkadeeri\\Zotero\\storage\\5L2IK2AG\\Nemergut et al. - 2013 - Patterns and Processes of Microbial Community Asse.pdf}
