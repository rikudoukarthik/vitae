@article{dutilleul2000,
  title = {The {{Mantel Test}} versus {{Pearson}}'s {{Correlation Analysis}}: {{Assessment}} of the {{Differences}} for {{Biological}} and {{Environmental Studies}}},
  shorttitle = {The {{Mantel Test}} versus {{Pearson}}'s {{Correlation Analysis}}},
  author = {Dutilleul, Pierre and Stockwell, Jason D. and Frigon, Dominic and Legendre, Pierre},
  year = {2000},
  journal = {Journal of Agricultural, Biological, and Environmental Statistics},
