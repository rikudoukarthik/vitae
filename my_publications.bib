@article{schmaljohann2018,
  title = {Proximate Mechanisms Affecting Seasonal Differences in Migration Speed of Avian Species},
  author = {Schmaljohann, Heiko},
  year = {2018},
  month = mar,
  journal = {Scientific Reports},
  volume = {8},
  number = {1},
  pages = {4106},
  publisher = {Nature Publishing Group},
  issn = {2045-2322},
  doi = {10.1038/s41598-018-22421-7},
  urldate = {2024-08-24},
  abstract = {Faster migration in spring than in autumn seems to be a common pattern in birds. This has been ultimately explained by seasonally different selection pressures. Variation in migration speed is proximately caused by adjusting travel speed (distance covered during flight) and/or stopover duration (times when birds rest and refuel). Yet, it remains unclear whether individual seasonal differences in migration speed match the common pattern and what the precise role of the proximate, behavioural mechanisms for adjusting migration speed is. By reviewing 64 studies of 401 tracks, I show that in waders, gulls, swifts, and songbirds speeds were significantly higher in spring, while the opposite was the case in waterfowl and owls. Thus, the ultimate mechanisms selecting for a faster migration in spring might not consistently act across bird groups. Breeding latitude, migration strategy, migration distance, flight style, body mass, and sex did not explain seasonal differences in speed. The ratio between spring and autumn total stopover duration of 257 bird tracks significantly negatively affected the seasonal migration speed ratio of the same individuals in a comparative analysis accounting for shared ancestry. Seasonal variation in stopover duration appears thus to be the main biological mechanism regulating seasonal differences in migration speed.},
  copyright = {2018 The Author(s)},
  langid = {english},
  keywords = {/unread,Animal migration,Behavioural ecology,Evolutionary ecology},
  file = {D:\ACADEMIA\Zotero Library\Schmaljohann_2018_Proximate mechanisms affecting seasonal differences in migration speed of avian.pdf}
}

