@article{vanderpluym2024,
  title = {Toward a Comparative Framework for Studies of Altitudinal Migration},
  author = {Vander~Pluym, David and Mason, Nicholas A.},
  year = {2024},
  month = sep,
  journal = {Ecology and Evolution},
  volume = {14},
