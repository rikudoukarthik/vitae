@article{saab1999,
  title = {Importance of Spatial Scale to Habitat Use by Breeding Birds in Riparian Forests: {{A}} Hierarchical Analysis},
  shorttitle = {Importance of {{Spatial Scale}} to {{Habitat Use}} by {{Breeding Birds}} in {{Riparian Forests}}},
  author = {Saab, Victoria},
