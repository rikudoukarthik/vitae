@article{inskipp2020,
  title = {Rare Birds in {{Nepal}}},
  author = {Inskipp, Carol and Baral, Hem Sagar and Acharya, Sanjib and Chaudhary, Hathan and Ghimire, Manshanta and Giri, Dinesh},
  year = {2020},
